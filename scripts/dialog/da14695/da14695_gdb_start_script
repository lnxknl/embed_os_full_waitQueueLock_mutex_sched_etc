define reset
	monitor reset 0
	monitor halt
end

set print pretty on

target remote :2331

monitor memU32 0x100c0050 = 1
monitor reset 0
monitor sleep 1
monitor reset 0
monitor memU32 0x50000024 = 0x000000D0

monitor reset 0
monitor memU32 0x50000024 = 0x000000D0

reset
reset

eval "monitor exec SetRTTAddr %p", &_SEGGER_RTT
